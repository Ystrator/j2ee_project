<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.devMode" value="true" />
    <package name="default"  extends="struts-default">
    
		<default-action-ref name="loginAction"></default-action-ref>
        <action name="loginAction"  class="loginAction">
            <result>/jsp/index.jsp</result>
        </action>
        <action name="registAction" class="registAction">
        	<result>/jsp/regist.jsp</result>
        </action>
        <action name="newsReleaseAction" class="newsReleaseAction">
        	<result>/jsp/newsRelease.jsp</result>
        </action>
        
        <action name="newsReadAction" class="newsReadAction">
        	<result>/jsp/newsRead.jsp</result>
        </action>
        
        
        
     </package>
</struts>